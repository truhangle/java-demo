<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>HelloJ2ee</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <!-- log4j日志配置监听 -->
  <!-- <listener>
    <listener-class>com.tjango.d.Log4jConfigListener</listener-class>
  </listener> -->
  
  <servlet>
     <servlet-name>zimi</servlet-name>
    <servlet-class>com.tjango.d.action.ZimiServlet</servlet-class>
  </servlet>
  <servlet-mapping>
     <servlet-name>zimi</servlet-name>
     <url-pattern>/zimi</url-pattern>
  </servlet-mapping>
  
</web-app>